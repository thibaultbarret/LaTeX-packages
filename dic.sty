% Composantes matrice de rotation x :
\newcommand{\matriceRotationX}{\ensuremath{\begin{bmatrix}
            1 &         0      &        0        \\
            0 &  \cosP{\alpha} &  -\sinP{\alpha} \\
            0 &  \sinP{\alpha} &  \phantMinus\cosP{\alpha} 
\end{bmatrix}}\xspace}


% Composantes matrice de rotation y :
\newcommand{\matriceRotationY}{\ensuremath{\begin{bmatrix}
            \phantMinus\cosP{\beta}  &  0 & \sinP{\beta} \\
                  0       &  1 &      0       \\
           -\sinP{\beta}  &  0 & \cosP{\beta}
\end{bmatrix}}\xspace}

% Composantes matrice de rotation z :
\newcommand{\matriceRotationZ}{\ensuremath{\begin{bmatrix}
            \cosP{\gamma} & -\sinP{\gamma} & 0 \\
            \sinP{\gamma} & \phantom{-} \cosP{\gamma} & 0 \\
                  0       &        0       & 1
\end{bmatrix}}\xspace}


% Composantes matrice de rotation x transposee : 
\newcommand{\compTransMatRx}{\ensuremath{\begin{bmatrix}
            1 &              0            &        0        \\
            0 &  \phantMinus\cosP{\alpha} &  \sinP{\alpha} \\
            0 &  -\sinP{\alpha}           &  \cosP{\alpha} 
\end{bmatrix}}\xspace}

% Composantes matrice de rotation y transposee : 
\newcommand{\compTransMatRy}{\ensuremath{\begin{bmatrix}
            \cosP{\beta}  &  0  &       -\sinP{\beta}    \\
                  0       &  1  &             0          \\
            \sinP{\beta}  &  0  & \phantMinus\cosP{\beta}
\end{bmatrix}}\xspace}

% Composantes matrice de rotation z transposee : 
\newcommand{\compTransMatRz}{\ensuremath{\begin{bmatrix}
            \phantMinus \cosP{\gamma} & \sinP{\gamma} & 0 \\
            -\sinP{\gamma}            & \cosP{\gamma} & 0 \\
                         0            &        0      & 1
\end{bmatrix}}\xspace}


