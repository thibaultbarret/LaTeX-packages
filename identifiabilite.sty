\newcommand{\espaceParametres}{\ensuremath{\boldsymbol{\Theta}}\xspace}
\newcommand{\nbParametres}{\ensuremath{N_{\theta}}\xspace}

% Jeu de parametres :
\newcommand{\jeuParametres}{\ensuremath{\ve{\theta}}\xspace}

\newcommand{\thetaInd}[1]{\ensuremath{\theta_{#1}}\xspace}
\newcommand{\thetaIndNumb}[1]{\ensuremath{\theta_{\Scale[0.6]{#1}}}\xspace}

\newcommand{\jeuParametresCompExpl}{\ensuremath{\bigl\{ \thetaIndNumb{1}, \: \thetaInd{n}, \: ..., \thetaIndNumb{\nbParametres} \bigr\}}\xspace}
\newcommand{\jeuParametresCompExplPert}{\ensuremath{\bigl\{ \thetaIndNumb{1}, \: \thetaInd{n} \times (1 + \epsilon), \: ..., \thetaIndNumb{\nbParametres} \bigr\}}\xspace}

%\newcommand{\jeuParametresCompExpl}{\ensuremath{\bigl\{ \thetaIndNumb{1}, \: \thetaInd{j}, \: ..., \theta_{\Scale[0.65]{1}}, \bigr\}}\xspace}

% Composantes jeu de parametres : 
\newcommand{\jeuParametresComp}{\ensuremath{ \bigl\{ \theta \bigr\} }\xspace}
\newcommand{\jeuParametresNoComp}[1]{\ensuremath{ \bigl\{ \theta \bigr\}_{\Scale[0.6]{#1}}}\xspace}

% Composantes jeu de parametres iteration : 
\newcommand{\jeuParametresIt}[1]{\ensuremath{ \jeuParametres^{#1} }\xspace}
\newcommand{\jeuParametresItComp}[1]{\ensuremath{ \bigl\{ \theta^{#1} \bigr\} }\xspace}

% Jeu de parametres initial :
\newcommand{\jeuParametresInitialNoComp}[1]{\ensuremath{ \bigl\{ \theta_{\Scale[0.6]{\textsc{ini}}} \bigr\}_{\Scale[0.6]{#1}}}\xspace}

% Jeu de parametres final :
\newcommand{\jeuParametresFinal}{\ensuremath{ \hat{\jeuParametres}  }\xspace}
\newcommand{\jeuParametresFinalComp}{\ensuremath{ \bigl\{ \hat{\theta} \bigr\} }\xspace}
\newcommand{\jeuParametresFinalNoComp}[1]{\ensuremath{ \bigl\{ \hat{\theta} \bigr\}_{\Scale[0.6]{#1}}}\xspace}


% Fonction cout : 
\newcommand{\ChiSquare}{\ensuremath{\chi^{2}}\xspace}

% Fonction cout avec les parametres : 
\newcommand{\ChiSquareTheta}{\ensuremath{\chi^{2} \bigl( \jeuParametres \bigr)}\xspace}

% Modele : 
\newcommand{\modele}{\ensuremath{\mathscr{M}}\xspace}

% Modele d'un jeu de parametres :
\newcommand{\modeleTheta}{\ensuremath{\modele \bigl( \jeuParametres \bigr)}\xspace}

\newcommand{\modeleThetaComp}{\ensuremath{\modele \bigl( \jeuParametresComp \bigr)}\xspace}

% Grande somme : 
\newcommand{\biggSum}[2]{\ensuremath{\mathop{\Scale[2.]{\sum}}_{#2}^{#1}\xspace}}

% Tres grande somme : 
\newcommand{\bigggSum}[2]{\ensuremath{\mathop{\Scale[2.5]{\sum}}_{#2}^{#1}\xspace}}

% Variable observable o : 
\newcommand{\oVar}{\ensuremath{\mathscr{o}}\xspace}

% Point p 
\newcommand{\pPoint}{\ensuremath{\mathscr{p}}\xspace}

% Instant i 
\newcommand{\iInstant}{\ensuremath{\mathscr{i}}\xspace}

