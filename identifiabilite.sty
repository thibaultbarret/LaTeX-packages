\newcommand{\vecYnum}{\ensuremath{\boldsymbol{y}_{\numInd}}\xspace}

\newcommand{\vecYnumComp}{\ensuremath{\bigl\{ y_{\numInd}  \bigr\}}\xspace}
\newcommand{\vecYnumCompTheta}{\ensuremath{\bigl\{ y_{\numInd} \bigl( \jeuParametres \bigr) \bigr\}}\xspace}

\newcommand{\vecYnumCompVar}[1]{\ensuremath{\bigl\{ y_{\numInd} \bigl( #1 \bigr) \bigr\}}\xspace}

