% Derivative of a signal with respect of θⱼ : ∂/∂θⱼ
\newcommand{\dSignaldThethaJ}[1]{\ensuremath{\dpar{#1}{\thetaInd{j}}}\xspace}

% Scaled derivative of a signal with respect of θⱼ : θⱼ x  ∂/∂θⱼ
\newcommand{\scdSignaldThethaJ}[1]{\ensuremath{\thetaInd{j} \times \dpar{#1}{\thetaInd{j}}}\xspace}

\newcommand{\sensibilitySignalThethaJScaledByMax}[1]{\ensuremath{\dfrac{\thetaInd{j}}{\max #1} \times \dpar{#1}{\thetaInd{j}}}\xspace}

% Sensitivity matrix : 
\newcommand{\matSensitivity}{\ensuremath{\mat{S}}\xspace}
\newcommand{\matSensitivityComp}{\ensuremath{\bigl[ S \bigr]}\xspace}

% Measure of sensitivity : 
\newcommand{\deltaMeasure}{\ensuremath{\delta^{\msrInd}_{j}}\xspace}


