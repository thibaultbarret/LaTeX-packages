% Vecteur observable : 
\newcommand{\vecObsComp}[1]{\ensuremath{\bigl\{ #1 \bigr\}}\xspace}

% Vecteur residus : 
\newcommand{\vecRes}{\ensuremath{\ve{r}}\xspace}

% Composantes vecteur residus  : 
\newcommand{\vecResComp}{\ensuremath{\vecObsComp{r}}\xspace}

% Vecteur residus adimensionne : 
\newcommand{\vecResAd}{\ensuremath{\ve{\overline{r}}}\xspace}

% Composantes vecteur residus adimensionne : 
\newcommand{\vecResAdComp}{\ensuremath{\vecObsComp{\overline{r}}}\xspace}



